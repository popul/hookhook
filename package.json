{
  "name": "hookhook",
  "version": "1.0.0",
  "description": "Git hooks for multi projects repository",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "commitmsg": "validate-commit-msg",
    "release": "standard-version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/popul/hookhook.git"
  },
  "keywords": [
    "git",
    "hook",
    "npm"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "author": "Paul Musso<paul@musso.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/popul/hookhook/issues"
  },
  "homepage": "https://github.com/popul/hookhook#readme",
  "devDependencies": {
    "conventional-changelog-cli": "^1.3.1",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.13.3",
    "standard-version": "^4.0.0",
    "validate-commit-msg": "^2.12.1"
  }
}
